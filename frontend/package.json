{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "routify -bD && rollup -c",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "autoprefixer": "^9.7.6",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "ppid-changed": "^1.0.1",
    "rollup": "^2.6.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-postcss": "^3.1.1",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "spassr": "^1.0.2",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.3.5"
  },
  "dependencies": {
    "@sveltech/routify": "^1.5.2",
    "bulma": "^0.8.2",
    "moment": "^2.24.0",
    "svelte": "^3.21.0",
    "svelte-frappe-charts": "^1.1.1"
  }
}
